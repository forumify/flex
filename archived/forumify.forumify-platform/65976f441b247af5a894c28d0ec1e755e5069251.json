                        "   */",
                        "  .addEntry('frontend', './assets/frontend.js')",
                        "  .addEntry('admin', './assets/admin.js')",
                        "  .splitEntryChunks()",
                        "  .enableStimulusBridge('./assets/controllers.json')",
                        "  .enableSingleRuntimeChunk()",
