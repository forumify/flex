{
    "manifests": {
        "forumify/forumify-platform": {
            "manifest": {
                "bundles": {
                    "Forumify\\ForumifyBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "assets/bootstrap.js",
                        "position": "after_target",
                        "target": "/\\.[jt]sx?$/\n));",
                        "requires": "symfony/stimulus-bundle",
                        "warn_if_missing": true,
                        "content": "\n  console.log('hi');\n"
                    }
                ]
            },
            "files": {
                "config/routes/forumify.yaml": {
                    "contents": [
                        "forumify:",
                        "    resource: '@ForumifyBundle/config/routes.yaml'",
                        "    name_prefix: forumify_",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "862811e1b628d199110e4d260c7a75f507e0827b"
        }
    }
}
